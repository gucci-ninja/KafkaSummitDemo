  "source": "pragma solidity ^0.5.0;\n\ncontract Patents {\n\n  struct Patent {\n    uint id;\n    string description;\n    address owner;\n  }\n\n  uint public numPatents = 0;\n  mapping(uint => Patent) public patents;\n\n  function newPatent(string memory _description) public {\n    patents[numPatents] = Patent(\n      numPatents,\n      _description,\n      msg.sender\n    );\n    numPatents++;\n  } \n}",
